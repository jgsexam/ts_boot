<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exam.mapper.PaperTypeScoreMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.exam.pojo.PaperTypeScoreDO">
        <id column="sc_id" property="scId" />
        <result column="sc_type" property="scType" />
        <result column="sc_paper" property="scPaper" />
        <result column="sc_score" property="scScore" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sc_id, sc_type, sc_paper, sc_score
    </sql>

</mapper>
